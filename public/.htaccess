RewriteEngine On
RewriteBase /

RewriteCond %{HTTP_HOST} !^casaflores.com.br$ [NC]
RewriteRule ^(.*)$ http://casaflores.com.br/$1 [L,R=301]

RewriteCond $1 !^(blogcasaflores)

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)\?*$ index.php?_route_=$1 [L,QSA]

RedirectMatch 301 /blogcasaflores http://casaflores.com.br/index.php?route=simple_blog/category&simple_blog_category_id=7

